{
  "name": "paps-app/paps-php-client",
  "description": "Paps API SDK for PHP",
  "license": "MIT",
  "keywords": [
    "paps",
    "api-client",
    "sdk"
  ],
  "type": "library",
  "homepage": "https://github.com/paps-app/paps-php-client",
  "authors": [
    {
      "name": "Kiamet MAVOUNGOU",
      "homepage": "https://github.com/Kiamet"
    }
  ],
  
    "require": {
      "php": ">=7.2 <8.2",
      "ext-curl": "*",
      "ext-json": "*",
      "ext-mbstring": "*",
      "apimatic/unirest-php": "^2.1",
      "apimatic/jsonmapper": "^3.0.0"
  },
  "require-dev": {
      "squizlabs/php_codesniffer": "^3.5",
      "phan/phan": "5.3.1",
      "phpunit/phpunit": "^7.5 || ^8.5"
  },
  "autoload": {
      "psr-4": {
          "PapsAPILib\\": "src/"
      }
  }
}
